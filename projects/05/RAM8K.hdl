CHIP RAM8K {
    IN in[16], load, address[13];
    OUT out[16];

    PARTS:
    DMux (in=load, sel=address[12], a=load0, b=load1);

    RAM4K (in=in, load=load0, address=address[0..11], out=previousWord0);
    RAM4K (in=in, load=load1, address=address[0..11], out=previousWord1);

    Mux16 (a=previousWord0, b=previousWord1, sel=address[12], out=out);
}